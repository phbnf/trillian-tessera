{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "sts:GetCallerIdentity"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:PassRole"
            ],
            "Resource": [
                "arn:aws:secretsmanager:us-east-1:864981736166:role:*",
                "arn:aws:iam::864981736166:role/ecsTaskExecutionRole",
                "arn:aws:iam::864981736166:role/ConformanceECSTaskRolePolicy"
            ]
        }
    ]
}